from Tkinter import *

class App:

    def __init__(self, master):
        
        self.exit = Button(master, text= "QUIT", fg="red", command=frame.quit)
        self.exit.grid(sticky= E)

        self.hello_world = Button(master, text= "HELLO", command= DISABLED)
        self.hello_world.grid(sticky= E)


root = Tk()
app = App(root)
root.mainloop()
